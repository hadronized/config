{
  "packager": "packer",
  "plugins": {
    "tjdevries/colorbuddy.vim": {},

    "romgrk/doom-one.vim": {},

    "sainnhe/sonokai": {
      "global_setup": {
        "sonokai_style": "andromeda",
        "sonokai_enable_italic": true,
        "sonokai_cursor": "red",
        "sonokai_transparent_background": false,
        "sonokai_menu_selection_background": "green",
        "sonokai_sign_column_background": "none",
        "sonokai_diagnostic_line_highlight": true,
        "sonokai_current_word": "grey background",
        "sonokai_better_performance": true
      }
    },

    "glepnir/zephyr-nvim": {
      "branch": "main"
    },

    "dracula/vim": {
      "rename": "dracula"
    },

    "challenger-deep-theme/vim": {
      "rename": "challenger-deep"
    },

    "drewtempelmeyer/palenight.vim": {
      "global_setup": {
        "palenight_terminal_italics": 1
      }
    },

    "Rigellute/shades-of-purple.vim": {},

    "archseer/colibri.vim": {},

    "embark-theme/vim": {
      "global_setup": {
        "embark_terminal_italics": 1
      }
    },

    "bkegley/gloombuddy": {},

    "sainnhe/edge": {
      "global_setup": {
        "edge_style": "neon",
        "edge_enable_italic": 1,
        "edge_cursor": "auto",
        "edge_menu_selection_background": "purple",
        "edge_diagnostic_text_highlight": 1,
        "edge_diagnostic_line_highlight": 1
      }
    },

    "joshdick/onedark.vim": {
      "global_setup": {
        "onedark_terminal_italics": 1
      }
    },

    "marko-cerovac/material.nvim": {
      "enable": true,
      "global_setup": {
        "material_style": "palenight",
        "material_flat_ui": 0,
        "material_italic_comment": 1,
        "material_italic_keywords": 1,
        "material_italic_functions": 1
      }
    },

    "nvim-lua/popup.nvim": {},

    "nvim-lua/plenary.nvim": {},

    "nvim-treesitter/nvim-treesitter": {},

    "~/dev/hop.nvim": {
      "rename": "hop",
      "config": {
        "keys": "etovxqpdygfblzhckisuran"
      }
    },

    "windwp/nvim-autopairs": {
      "enable": false,
      "config": {}
    },

    "preservim/tagbar": {},

    "tpope/vim-commentary": {},

    "mfussenegger/nvim-dap": {},

    "MattesGroeger/vim-bookmarks": {
      "global_setup": {
        "bookmark_sign": "♥",
        "bookmark_highlight_lines": 1
      }
    },

    "hrsh7th/nvim-compe": {
      "rename": "compe",
      "config": {
        "enabled": true,
        "autocomplete": true,
        "debug": false,
        "min_length": 1,
        "preselect": "enable",
        "throttle_time": 80,
        "source_timeout": 200,
        "incomplete_delay": 400,
        "max_abbr_width": 100,
        "max_kind_width": 100,
        "max_menu_width": 100,
        "documentation": true,
        "source": {
          "path": true,
          "buffer": true,
          "calc": true,
          "vsnip": false,
          "nvim_lsp": true,
          "nvim_lua": true,
          "spell": true,
          "tags": true,
          "snippets_nvim": false,
          "treesitter": false,
          "ultisnips": true
        }
      }
    },

    "romgrk/nvim-treesitter-context": {
      "enable": false,
      "depends": ["nvim-treesitter/nvim-treesitter"]
    },

    "kyazdani42/nvim-tree.lua": {
      "rename": "nvim-tree",
      "global_setup": {
        "nvim_tree_side": "left",
        "nvim_tree_width": 40,
        "nvim_tree_ignore": [".git", ".target"],
        "nvim_tree_auto_open": 0,
        "nvim_tree_auto_close": 1,
        "nvim_tree_quit_on_open": 0,
        "nvim_tree_follow": 1,
        "nvim_tree_indent_markers": 1,
        "nvim_tree_hide_dotfiles": 0,
        "nvim_tree_git_hl": 1,
        "nvim_tree_root_folder_modifier": ":.",
        "nvim_tree_tab_open": 0,
        "nvim_tree_allow_resize": 1,
        "nvim_tree_show_icons": {
          "git": 1,
          "folders": 1,
          "files": 1
        },
        "nvim_tree_icons": {
          "default": "",
          "symlink": "",
          "git": {
            "unstaged": "✗",
            "staged": "✓",
            "unmerged": "",
            "renamed": "➜",
            "untracked": "★"
          },
          "folder": {
            "default": "",
            "open": ""
          },
        }
      },
      "config": {
        "keybindings": {
          "<CR>": "edit",
          "o": "cd",
          "<C-v>": "vsplit",
          "<C-o>": "split",
          "<C-t>": "tabnew",
          "<BS>": "close_node",
          "<S-CR>": "close_node",
          "<Tab>": "preview",
          "I": "toggle_ignored",
          ".": "toggle_dotfiles",
          "R": "refresh",
          "a": "create",
          "d": "remove",
          "r": "rename",
          "<C-r>": "full_rename",
          "x": "cut",
          "y": "copy",
          "p": "paste",
          "c": "dir_up",
          "q": "close"
        }
      }
    },

    "tpope/vim-fugitive": {},

    "airblade/vim-gitgutter": {
      "global_setup": {
        "gitgutter_map_keys": false,
        "gitgutter_max_signs": 10000,
        "gitgutter_sign_added": "▎",
        "gitgutter_sign_modified": "▎",
        "gitgutter_sign_removed": "▁",
        "gitgutter_sign_removed_first_line": "▔",
        "gitgutter_sign_modified_removed": "▎",
        "gitgutter_highlight_linenrs": 0,
        "gitgutter_override_sign_column_highlight": false
      }
    },

    "tveskag/nvim-blame-line": {
      "global_setup": {
        "blameLineGitFormat": "   %an | %ar | %s"
      }
    },

    "TimUntersberger/neogit": {
      "enable": false
    },

    "pwntester/octo.nvim": {
      "enable": false,
    },

    "rafcamlet/nvim-luapad": {},

    "euclidianAce/BetterLua.vim": {},

    "neovim/nvim-lspconfig": {},

    "nvim-lua/lsp_extensions.nvim": {
      "rename": "lsp_extensions",
      "config": {
        "rust_analyzer_inlay_hints": {
          "prefix": " » ",
        }
      },
    },

    "nvim-lua/lsp-status.nvim": {
      "rename": "lsp-status",
    },

    "honza/vim-snippets": {
      "global_setup": {
        "snips_author": "Dimitri Sabadie"
      }
    },

    "SirVer/ultisnips": {
      "global_setup": {
        "UltiSnipsExpandTrigger": "<tab>",
        "UltiSnipsJumpForwardTrigger": "<tab>",
        "UltiSnipsJumpBackwardTrigger": "<S-tab>"
      }
    },

    "vim-test/vim-test": {},

    "rcarriga/vim-ultest": {
      "depends": ["janko/vim-test"],
      "after": ":UpdateRemotePlugins"
    },

    "universal-ctags/ctags": {},

    "nvim-telescope/telescope.nvim": {},

    "nvim-telescope/telescope-fzy-native.nvim": {
      "enable": false,
      "depends": ["nvim-telescope/telescope.nvim"]
    },

    "nvim-telescope/telescope-fzf-native.nvim": {
      "depends": ["nvim-telescope/telescope.nvim"],
      "after": "make",
    },

    "norcalli/nvim-colorizer.lua": {
      "rename": "colorizer",
      "config": {}
    },

    "ryanoasis/vim-devicons": {},

    "kyazdani42/nvim-web-devicons": {},

    "yamatsum/nvim-web-nonicons": {
      "enable": false,
      "depends": ["kyazdani42/nvim-web-devicons"]
    },

    "lukas-reineke/indent-blankline.nvim": {
      "branch": "lua",
      "enable": true,
      "global_setup": {
        "indent_blankline_char": "│",
        "indent_blankline_use_treesitter": true,
        "indent_blankline_show_current_context": false,
        "indent_blankline_show_trailing_blankline_indent": false,
        "indent_blankline_context_highlight": "SpecialComment"
      }
    },

    "glepnir/galaxyline.nvim": {
      "branch": "main",
      "enable": false
    },

    "petrbroz/vim-glsl": {},

    "towolf/vim-helm": {},

    "idris-hackers/idris-vim": {
      "global_setup": {
        "idris_indent_if": 3,
        "idris_indent_let": 4,
        "idris_indent_where": 6,
        "idris_indent_do": 3,
        "idris_indent_rewrite": 8
      }
    },

    "plasticboy/vim-markdown": {
      "global_setup": {
        "vim_markdown_new_list_item_indent": 2
      }
    },

    "mzlogin/vim-markdown-toc": {},

    "SidOfc/mkdx": {
      "enable": false,
    },

    "rust-lang/rust.vim": {
      "global_setup": {
        "rust_recommended_style": 0
      }
    },

    "vim-scripts/SyntaxAttr.vim": {}
  }
}
